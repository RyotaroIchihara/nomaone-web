---
import BaseLayout from '../../layouts/BaseLayout.astro';
const pageTitle = '指定特定相談支援';
const pageDescription = 'サービス利用計画（計画案）の作成、モニタリング、必要時の見直し・改善まで継続的に実施し、適切な障害福祉サービス利用と地域生活・社会参加を支えます。';
---
<BaseLayout title={pageTitle} description={pageDescription}>
  <h1 class="text-2xl font-bold mb-4">{pageTitle}</h1>
  <p>{pageDescription}</p>
</BaseLayout>
